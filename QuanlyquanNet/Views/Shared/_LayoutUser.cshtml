      @{
        var hoTen = Context.Session.GetString("HoTen");
      }
        @if (string.IsNullOrEmpty(hoTen))
         {
           <!-- Nếu chưa đăng nhập -->
           <a class="cta-btn" asp-controller="User" asp-action="Login">Đăng nhập</a>
         }
         else
          {
             <!-- Nếu đã đăng nhập -->
             <div class="d-flex align-items-center gap-2">
                    <a asp-controller="Customer" asp-action="Profile" class="text-white d-flex align-items-center text-decoration-none">
                        <i class="bi bi-person-circle fs-4 me-1"></i> @hoTen
                    </a>
                  <form method="post" asp-controller="User" asp-action="Logout" style="display:inline;">
